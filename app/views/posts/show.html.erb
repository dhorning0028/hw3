<h1>New Post</h1>

<%= form_with model: @post do |form| %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "first_name" %>
    <%= form.label "last_name" %>
    <%= form.text_field "last_name" %>
  </p>
  <p>
    <%= form.hidden_field "company_id" %>
  </p>
  <%= form.submit %>
<% end %>